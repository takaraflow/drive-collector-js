# 调试配置文件
# 将此内容添加到你的 .env 文件中

# 启用详细日志
NODE_DEBUG=drive-collector:*
DEBUG=drive-collector:*

# 启用性能监控
PERFORMANCE_LOGGING=true

# 启用任务跟踪
TASK_DEBUG=true

# 启用 Telegram 调试
TG_DEBUG=true

# 启用缓存调试
CACHE_DEBUG=true

# 启用数据库调试
D1_DEBUG=true

# 启用 QStash 调试
QSTASH_DEBUG=true

# 日志级别 (debug, info, warn, error)
LOG_LEVEL=debug